				<key>ActionName</key>
				<string>Run AppleScript</string>
				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>global target_folderglobal tab_namesglobal NAME_MAINglobal NAME_SBT_CONTROLglobal NAME_SCALA_COMPILEglobal NAME_COFFEE_MAINglobal NAME_COFFEE_TESTglobal NAME_KARMAon run args	-- Initialize target folder	set target_folder to item 1 of args		-- Initialize tab names	set tab_names to {}	set tab_names to tab_names &amp; {"Main"}	set tab_names to tab_names &amp; {"Sbt Control"}	set tab_names to tab_names &amp; {"Scala Compile"}	set tab_names to tab_names &amp; {"Coffee Main"}	set tab_names to tab_names &amp; {"Coffee Test"}	set tab_names to tab_names &amp; {"Karma"}		-- Initialize tab names keys		set NAME_MAIN to 1	set NAME_SBT_CONTROL to 2	set NAME_SCALA_COMPILE to 3	set NAME_COFFEE_MAIN to 4	set NAME_COFFEE_TEST to 5	set NAME_KARMA to 6		-- Operate the terminal	tell application "iTerm"		tell the current terminal						repeat with i from 1 to count of tab_names				set tab_name to item i of tab_names				set cmds to my commands_to_execute(i)								set name of current session to tab_name								repeat with cmd in cmds					tell current session to write text cmd				end repeat								if i &lt; (count of tab_names) then					launch session ""				end if			end repeat					end tell	end tellend runon commands_to_execute(idx)	-- Commons	set res to {}	set res to res &amp; {"cd " &amp; target_folder}		-- Custom for each name	if idx is NAME_MAIN then			else if idx is NAME_SBT_CONTROL then		set res to res &amp; {"sbt"}		set res to res &amp; {"container:start"}		set res to res &amp; {"test"}			else if idx is NAME_SCALA_COMPILE then		set res to res &amp; {"sbt"}		set res to res &amp; {"~;copy-resources;aux-compile"}			else if idx is NAME_COFFEE_MAIN then		set coffee_out_main to "src/main/webapp/assets/js/compiled"		set coffee_in_main to "src/main/coffee"		set res to res &amp; {coffee_compile(coffee_in_main, coffee_out_main)}			else if idx is NAME_COFFEE_TEST then		set coffee_in_test to "src/test/coffee"		set coffee_out_test to "src/test/js"		set res to res &amp; {coffee_compile(coffee_in_test, coffee_out_test)}			else if idx is NAME_KARMA then		set res to res &amp; {"karma start karma.conf.coffee"}			end if		resend commands_to_executeon coffee_compile(in_folder, out_folder)	"coffee -wo " &amp; out_folder &amp; " -c " &amp; in_folderend coffee_compile</string>
