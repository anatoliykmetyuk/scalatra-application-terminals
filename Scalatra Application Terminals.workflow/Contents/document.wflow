				<dict>
					<key>source</key>
					<string>on run args	-- Initialize target folder	tell application "Finder" to set target_folder to URL of item 1 of item 1 of args		set target_folder_chars to characters 8 thru -1 of target_folder	set target_folder to ""		repeat with c in target_folder_chars		set target_folder to target_folder &amp; c	end repeat		target_folderend run</string>
